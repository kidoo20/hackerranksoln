#include<stdio.h>
#include<stdlib.h>
int main()
{
    int i,n,d,k=0,temp,p;
    int a[100];
    scanf("%d %d",&n,&d);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    d=d%n;
    for(i=d;i<n;i++)
    {
        printf("%d ",a[i]);
    }
    for(i=0;i<d;i++)
    {
        printf("%d ",a[i]);
    }
    return 0;
    
}
